# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                                  controllers.HomeController.index
GET     /about                           controllers.HomeController.about
GET     /contact                          controllers.HomeController.contact
GET     /donation                         controllers.HomeController.donation
GET     /membership                         controllers.HomeController.membership
GET     /bookscategory                       controllers.HomeController.bookscategory
GET     /playtime                            controllers.HomeController.playtime
GET     /morenike                          controllers.HomeController.morenike
GET     /contact                          controllers.HomeController.contact







# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
